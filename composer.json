{
    "name": "elijahcruz/avatar",
    "description": "Generate an avatar for your users using Gravatar and more",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Elijahcruz\\Avatar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elijahcruz\\Avatar\\Tests\\": "test/"
        }
